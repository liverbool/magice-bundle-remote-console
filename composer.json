{
    "name": "magice/remote-console-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Symfony bundle for https://phpconsole.com",
    "homepage": "https://github.com/liverbool/magice-remote-console-bundle",
    "authors": [
        {
            "name": "ãƒ„ Liverbool",
            "email": "nukboon@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "library",
            "package": {
                "name": "phpconsole/phpconsole",
                "version": "master",
                "source": {
                    "url": "https://github.com/phpconsole/phpconsole.git",
                    "type": "git",
                    "reference": "master-dev"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "phpconsole/phpconsole": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Magice\\Bundle\\RemoteConsoleBundle": "src/"
        },
        "classmap" : ["vendor/phpconsole/phpconsole", "phpconsole.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    }
}